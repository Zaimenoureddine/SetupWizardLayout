<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.meiji.setupwizardDemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.meiji.setupwizardDemo.FirstActivity"
            android:theme="@style/SuwThemeMaterial.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.meiji.setupwizardDemo.SecondActivity"
            android:theme="@style/SuwThemeMaterial.Light"/>

        <activity
            android:name="com.meiji.setupwizardDemo.ThirdActivity"
            android:theme="@style/SuwThemeMaterial"/>
    </application>

</manifest>